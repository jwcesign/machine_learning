x=imread('m.png');
re=zeros(262144,1); 
for i=1:512
for j=1:512
tmp = 0;
tmp = sum(x(i,j,:));
re(((i-1)*512+j))=tmp/3;
end;
end;
fid = fopen('k.txt','wt')
for i=1:262144
    if re(i)==255
        fprintf(fid,'%d',0);
        if mod(i,512)==0
            fprintf(fid,'\n');
        end
    else
        fprintf(fid,'%d',1);
    end
end
